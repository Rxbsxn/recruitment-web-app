<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>    
    <td><%= user.gender_info %></td>
    <td><%= user.age_info %></td>
    <td><%= user.interests_info %></td>
    <td><%= user.can_delete %></td>
    <td><%= link_to '', send_mail_mails_path(user_id: user.id), method: :post, class: 'glyphicon glyphicon-envelope' %></td>
    <td><%= user.export_csv %></td>
  </tr>
<% end %>
